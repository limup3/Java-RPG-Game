package 클래스;

public class MyThread extends Thread {

	
	
	
	
	@Override
	public void run() {
	
		for(int i =0 ; i<3 ; i++)
		{
			System.out.println("aa");
		
			
			try {
				
				
				Thread.sleep(1000);
			} catch (Exception e) {
				System.out.println("오류가 발생했습니다 : "+e.getMessage());
			}
			
		}
		
	}

}
