package 자바3주차;

public class 연습3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int num = 2;
		
	for(int i=0 ; i<num+3 ; i++)
	{
		for(int k=0; k<=i; k++)
		{
			System.out.print("aaaaaa");
			
			if(i>=2)
			{
				break;
			}
		}
		
		System.out.println();
	}
	
	
	}

}
