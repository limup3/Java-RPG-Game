package Å¬·¡½º;

public class MyThread implements Runnable {

	String str[];
	public MyThread(String str[]) {
		this.str = str;
	}
	
	
	public void run() {
		for( int i = 0 ; i<str.length ; i++)
		{
			System.out.println(str[i]);
			
			try {
				Thread.sleep(100);
			} catch (Exception e) {
				// TODO: handle exception
			}
		}
	}
}
