package 자바3주차;

public class 연습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 1; 

		System.out.print("aa");
		for(int i =0; i<num+3 ; i++)
		{
			
			for(int j =0; j<i ; j++)
			{
				System.out.print("aaaaaa");
			}
			for(int k = 0 ; k<i ; k++)
			{
				if(k==1)
				{
					System.out.print("bbbb");

				}
			}

			System.out.println();
		}
		
	}

}
