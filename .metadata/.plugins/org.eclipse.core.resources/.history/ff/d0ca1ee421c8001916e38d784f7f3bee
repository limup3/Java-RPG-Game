package 자바3주차;

public class 연습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 2; //12 18 24

		for(int A=0; A<num+3; A++)
		{
			for(int B=num+2; B>A; B--)
			{
				System.out.print("ab");
			}
			for(int D=0; D<A; D++)
			{
				if(D!=num+3)
				System.out.print("cd");
			}
			for(int C=num+3; C>A; C--)
			{
				
				if(C!=A+1)
				{
					System.out.print("e");
				}
			}
			System.out.println("");
		}
		
		
	}

}
