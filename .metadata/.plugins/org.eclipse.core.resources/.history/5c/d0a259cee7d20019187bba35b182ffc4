				"                              eee    eee                       zeeEeeeeeK                    eez         ee                   ",
				"                              eee     ueeD                     zee                           eee         ee                   ",
				"                              Eee      #ee                     Wee                            eey        ee                   ",
				"                            eKeeeue     eeeXe                e5eeey#ee                        5eeeK    Deee                   ",
				"                            eeeeeee5    eeeee                eeeeeeeee                          eeeeeeeeEee                   ",
				
		};
		for(int i =0 ; i<Intro.length ; i++)
		{
			System.out.println(Intro[i]);
				
			try {
							
				Thread.sleep(10);
			} catch (Exception e) {
				System.out.println("오류가 발생했습니다 : "+e.getMessage());
			}
			
		}
		
	}

	

}

class MyThread2 extends Thread {
	String Tutorial[] = {						" Xeeeeeeeeeeeee                                                        eeu                 9eee      ", 
												" ee     e     eu                   ee                                  ee                    ee      ", 
												" e     ue     Ey                   De                                                       ee       ", 
												"       ue        yez#     9G#W   ezeeyee      #eeeeW     e9D  Deeee   e9#       zeeeeD       ee      ", 
												"       ue          ee       ee     ze       eey    eeW    ,eeeX        Weu    eeD    ee      ee      ", 
												"       ue          ee       eG     #e      ee        ey    ee           e     XK      eE     ee      ", 
												"       ue          ee       eG     9e     ue         ee    e            e       5eeee9e#     ee      ", 
												"       ue          ee       eG     9e     Ke         ee    e            e     eez     e#     ee      ", 
												"       ue          9e       eE     Ee      ee        ez    e            e    ee       e#     ee      ", 
												"        e           ee    Weee     ye       ee     Wee     e            e    Ee     Kee#     ee      ", 
												"     Weeeee          eeeeee         eeeeD    yeeeeee     eeeeeu       eeeee,  XeeeeeG eeeX eeeee9    "}
	
	public void run () {
		for (int i = 0; i<Tutorial.length ; i++)
		{
			System.out.println(Tutorial[i]);
		}
		
		try {
			Thread.sleep(500);
		} catch (Exception e) {
			System.out.println("오류가 발생했습니다 : "+e.getMessage());
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
}
