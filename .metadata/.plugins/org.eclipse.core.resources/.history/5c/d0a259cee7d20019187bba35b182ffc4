package 클래스;

public class MyThread extends Thread {


	
	
	
	@Override
	public void run() {
		String Intro[] = {
				"                                                                                                                                                      ",
				"    eeeeeeeK                                                                                                                                          ",
				"  eeeeeeeeeW                                                                                                                                          ",
