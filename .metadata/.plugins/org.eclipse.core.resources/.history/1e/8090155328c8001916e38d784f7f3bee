package 자바3주차;

public class 연습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 2; 

		
		for(int i =0; i<num+3 ; i++)
		{
			
			for(int j =0; j<1 ; j++)
			{
				System.out.print("aa");				
			}
			
			
			for(int k = 1; k<num+1 ; k++)
			{
				
				System.out.print("bbbbbb");
				
			}
			System.out.println();
		}
		
	}

}
