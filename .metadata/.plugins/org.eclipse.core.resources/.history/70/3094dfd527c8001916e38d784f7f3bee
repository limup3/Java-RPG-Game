package 자바3주차;

public class 연습 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int num = 1; 

		
		for(int i =0; i<num+3 ; i++)
		{
			
			for(int j =0; j<1 ; j++)
			{
				System.out.print("aa");				
			}
			
			if(i==1)
			{
				System.out.print("aaaaaa");
			}
			for(int k = 0; k<1 ; k++)
			{
				
				
			}
			System.out.println();
		}
		
	}

}
